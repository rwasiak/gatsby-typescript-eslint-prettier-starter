{
  "name": "gatsby-typescript-eslint-prettier-starter",
  "private": true,
  "description": "Gatsby starter with typescript, eslint, prettier, styled-components",
  "version": "0.1.0",
  "author": "Remigiusz Wasiak",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.21.8",
    "gatsby-image": "^2.3.0",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-sharp": "^2.5.1",
    "gatsby-plugin-styled-components": "^3.2.0",
    "gatsby-plugin-typescript": "^2.3.0",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-sharp": "^2.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rwasiak/gatsby-typescript-eslint-prettier-starter"
  }
}
